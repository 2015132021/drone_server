<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.drone_project_geekcc">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Drone_projectgeekcc"
        android:usesCleartextTraffic="true">
        <activity android:name=".LoglistActivity"></activity>
        <activity android:name=".MyinfoActivity" />
        <activity android:name=".RentActivity" />
        <activity android:name=".MainActivity02" />
        <activity
            android:name=".LoadingActivity"
            android:theme="@style/Theme.loading" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>